{
    "name": "project-kiyo",
    "version": "1.2.0",
    "description": "A multipurpose discord.js bot with slash commands and API integrations",
    "main": "src/index.js",
    "type": "commonjs",
    "scripts": {
        "start": "node src/index.js",
        "dev": "nodemon --ignore 'logs/*' src/index.js",
        "format": "prettier --write .",
        "test": "echo \"No tests specified yet\" && exit 0",
        "update": "npm update && npm audit fix",
        "cleanup": "rm -rf logs/*",
        "prepare": "husky"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/yourusername/project-kiyo.git"
    },
    "keywords": [
        "discord",
        "discord-bot",
        "discord.js",
        "bot",
        "slash-commands",
        "api-integration"
    ],
    "author": {
        "name": "Your Name",
        "email": "your.email@example.com",
        "url": "https://yourwebsite.com"
    },
    "license": "MIT",
    "engines": {
        "node": ">=18.0.0",
        "npm": ">=8.0.0"
    },
    "dependencies": {
        "@discordjs/opus": "^0.10.0",
        "@discordjs/voice": "^0.18.0",
        "@google/generative-ai": "^0.21.0",
        "@iamtraction/google-translate": "^2.0.1",
        "axios": "^1.6.2",
        "boxen": "^8.0.1",
        "chalk": "^4.1.2",
        "discord-player": "^7.1.0",
        "discord.js": "^14.21.0",
        "dotenv": "^16.3.1",
        "ffmpeg-static": "^5.2.0",
        "figures": "^6.1.0",
        "googleapis": "^144.0.0",
        "he": "^1.2.0",
        "jsonwebtoken": "^9.0.2",
        "mathjs": "^13.1.1",
        "moment": "^2.30.1",
        "mongoose": "^8.7.1",
        "ms": "^2.1.3",
        "node-cache": "^5.1.2",
        "play-dl": "^1.9.7",
        "ytdl-core": "^4.11.5"
    },
    "devDependencies": {
        "eslint": "^9.28.0",
        "husky": "^9.1.7",
        "lint-staged": "^15.2.0",
        "nodemon": "^3.1.10",
        "prettier": "3.2.5"
    },
    "lint-staged": {
        "*.js": [
            "eslint --fix",
            "prettier --write"
        ],
        "*.{json,md}": "prettier --write"
    },
    "nodemonConfig": {
        "ignore": [
            "node_modules/*",
            "logs/*"
        ],
        "delay": "1000",
        "env": {
            "NODE_ENV": "development"
        }
    }
}
